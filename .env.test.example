# Test Environment Variables
# Copy this file to .env.test and fill in your test values

# Supabase Configuration (Test/Staging)
VITE_SUPABASE_URL=https://your-test-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-test-anon-key-here

# Secrets Encryption (Generate with: openssl rand -base64 32)
SECRETS_ENCRYPTION_KEY=your-32-byte-base64-encoded-key-here

# Redis/Upstash (Test instance)
UPSTASH_REDIS_REST_URL=https://your-test-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-test-redis-token-here

# Optional: Error Tracking (Sentry)
VITE_SENTRY_DSN=https://your-test-sentry-dsn@sentry.io/project-id

# Optional: Analytics
VITE_POSTHOG_KEY=your-test-posthog-key-here
VITE_POSTHOG_HOST=https://app.posthog.com

# Test-specific flags
NODE_ENV=test
VITE_ENABLE_DEBUG_LOGS=true
VITE_MOCK_API_CALLS=false
